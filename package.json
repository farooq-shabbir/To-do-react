{
  "name": "todo-list-app",
  "version": "1.0.0",
  "description": "A simple Todo List application with CRUD operations",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run --coverage",
    "test:watch": "vitest",
    "lint": "eslint src --ext js,jsx",
